{
    "name": "c4ys/yii2-attachments",
    "description": "yii2 文件上传扩展",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","attachment","file","upload"],
    "license": "BSD-4-Clause",
    "homepage": "https://github.com/c4ys/yii2-attachments",
    "authors": [
        {
            "name": "Alimzhan Abuov aka Nemmo",
            "email": "alimzhan.abu@yandex.kz"
        },
        {
            "name": "Alexander Palchikov aka AxelPAL",
            "email": "axelpal@gmail.com"
        },
        {
            "name": "Damian Gomez aka Badbreze",
            "email": "info@divenock.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.0",
        "kartik-v/yii2-widget-fileinput": "~1.0.0",
        "himiklab/yii2-colorbox-widget": "*",
        "yiisoft/yii2-jui": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "file\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
